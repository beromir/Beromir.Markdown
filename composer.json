    "require": {
        "neos/neos": "^8.0",
        "php": "^8.1",
        "league/commonmark": "^2.3.2"
    },
    "autoload": {
        "psr-4": {
            "Beromir\\Markdown\\": "Classes/"
        }
    },
    "authors": [
        {
            "name": "Henrik Rei√üig",
            "role": "Developer"
        }
